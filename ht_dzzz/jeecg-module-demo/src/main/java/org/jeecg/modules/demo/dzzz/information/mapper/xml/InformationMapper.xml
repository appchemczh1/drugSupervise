<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.demo.dzzz.information.mapper.InformationMapper">


    <select id="getZsbh" parameterType="java.lang.String" resultType="java.util.Map">
        select zsbh from cert_info where shxydm = #{shxydm}
    </select>

    <select id="selectZsCount" parameterType="java.lang.String" resultType="java.lang.Integer">
        select count(1) from cert_info where shxydm = #{shxydm}
    </select>

</mapper>