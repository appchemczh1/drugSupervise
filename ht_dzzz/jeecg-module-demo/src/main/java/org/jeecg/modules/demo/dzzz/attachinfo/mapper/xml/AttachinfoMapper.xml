<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.demo.dzzz.attachinfo.mapper.AttachinfoMapper">

    <select id="findAttachinfoByZSid" parameterType="java.lang.String" resultType="org.jeecg.modules.demo.dzzz.attachinfo.entity.Attachinfo" >
        SELECT
            *
        FROM
            ATTACHINFO a
        WHERE
            ZSID = #{zsid}
          AND STATUS = #{status}
          AND "TYPE" = #{type}
        ORDER BY
            CREATE_TIME DESC
    </select>
</mapper>